#include<stdio.h>
int main(){
    int Q=0,W=0,E=0,ct;
    char ch[1000001];
    for(int i=0;i<1000001;i++){
        scanf("%c",&ch[i]);
    }
    for(int i=0;i<1000001;i++){
        if(ch[i]=='R'){
            if(i<=3)
                printf("FFFFFFFK\n");
            else{
                for(int j=i-1;j>=0;j--){
                    if(ch[j]=='Q')
                        Q += 1;
                    if(ch[j]=='W')
                        W += 1;
                    if(ch[j]=='E')
                        E += 1;
                    ct = Q + W + E;
                    if(ct==3)
                        break;
                }
                
            }
        }
    }
}
